{
  "name": "Book-A-Meal",
  "version": "1.0.0",
  "description": "Book-A-Meal is an application that allows customers to make food orders and helps the food vendor know what the customers want to eat.",
  "main": "index.js",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "express": "^4.16.3",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "mocha": "^5.1.1"
  },
  "scripts": {
    "start": "nodemon --exec babel-node server/app.js",
    "build": "babel ./server",
    "test": "mocha --compilers js:babel-core/register server/test/index.js",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnadesina/Book-A-Meal.git"
  },
  "keywords": [],
